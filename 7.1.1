def count_characters(string):
    
    frequency = {}

   
    for char in string:
       
        if char in frequency:
            frequency[char] += 1
        else:
            frequency[char] = 1

   
    for char, count in frequency.items():
        print(f"Character '{char}' occurs {count} times")


input_string = input("Enter a string: ")
count_characters(input_string)
