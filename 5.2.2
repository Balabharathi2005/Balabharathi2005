def main():
  n = int(input("Enter how many values you want to read : "))
  arr = [0] * n
  for i in range(n):
    arr[i] = int(input(f"Enter the value of a[{i}] : "))

  # Bubble sort algorithm
  for i in range(n - 1):
    for j in range(i + 1, n):
      if arr[i] > arr[j]:
        temp = arr[i]
        arr[i] = arr[j]
        arr[j] = temp

  print("Array sorted in ascending order = ", end="")
  for i in range(n):
    print(arr[i], end=" ")
  print("\n")

if __name__ == "__main__":
  main()
